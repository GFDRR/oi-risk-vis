{
  "version": 8,
  "name": "Overview",
  "center": [
    -64.405087,
    -34.919628
  ],
  "height": "600px",
  "zoom": 4,
  "bearing": 0,
  "pitch": 0,
  "sources": {
    "boundaries": {
      "type": "vector",
      "url": "mbtiles://{boundaries}"
    }
  },
  "glyphs": "{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#c6e1ff"
      }
    },
    {
      "id": "landuse-neighbour",
      "type": "fill",
      "source": "boundaries",
      "source-layer": "admin_0_boundaries",
      "paint": {
        "fill-color": "#e0e0e0"
      }
    },
    {
      "id": "landuse-neighbour-borders",
      "type": "line",
      "source": "boundaries",
      "source-layer": "admin_0_boundaries",
      "paint": {
        "line-color": "#ffffff",
        "line-width": 2
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "boundaries",
      "source-layer": "admin_1_boundaries",
      "paint": {
        "fill-color": "#d2d2d2"
      },
      "minzoom": 4
    },
    {
      "id": "landuse-highlight",
      "type": "fill",
      "source": "boundaries",
      "source-layer": "admin_1_boundaries",
      "paint": {
        "fill-color": "#7c7c7c"
      },
      "filter": [
        "all",
        [
          "==",
          "name",
          "Buenos Aires"
        ]
      ],
      "minzoom": 4
    },
    {
      "id": "landuse-borders",
      "type": "line",
      "source": "boundaries",
      "source-layer": "admin_1_boundaries",
      "paint": {
        "line-color": "#ffffff",
        "line-width": 2
      },
      "minzoom": 4
    },
    {
      "id": "water",
      "type": "fill",
      "source": "boundaries",
      "source-layer": "physical_lakes",
      "paint": {
        "fill-color": "#c6e1ff"
      }
    },
    {
      "id": "country_labels",
      "type": "symbol",
      "source": "boundaries",
      "source-layer": "admin_0_boundaries_labels",
      "layout": {
        "text-field": "{name}",
        "symbol-placement": "point"
      },
      "filter": [
        "all",
        [
          "!=",
          "name",
          "Argentina"
        ]
      ]
    },
    {
      "id": "country_labels_argentina",
      "type": "symbol",
      "source": "boundaries",
      "source-layer": "admin_0_boundaries_labels",
      "layout": {
        "text-field": "{name}",
        "symbol-placement": "point"
      },
      "maxzoom": 4,
      "filter": [
        "all",
        [
          "==",
          "name",
          "Argentina"
        ]
      ]
    },
    {
      "id": "provinces_labels",
      "type": "symbol",
      "source": "boundaries",
      "source-layer": "admin_1_boundaries_labels",
      "layout": {
        "text-field": "{name}",
        "symbol-placement": "point"
      },
      "minzoom": 4
    }
  ]
}